{
  "name": "tritonse.github.io",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "generate-images": "ts-node images/generate.ts",
    "dev": "concurrently \"next dev\" \"chokidar \\\"images/*/**\\\" -c \\\"npm run generate-images\\\"\" \"npm run generate-images\"",
    "build": "npm run generate-images && next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint-fix": "(eslint --fix --cache --report-unused-disable-directives . || true) && prettier --write .",
    "lint-check": "tsc && eslint --cache --report-unused-disable-directives . && prettier --check .",
    "link-test": "linkinator out/ --skip 'dynamicsperformanceteam.com|linkedin.com|ssa.ucsd.edu|jointheleague.org|sdcta.org'",
    "prepare": "husky install .husky"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.2.3",
    "@tritonse/tse-constellation": "^0.0.16",
    "@types/luxon": "^3.4.2",
    "@types/mdx": "^2.0.13",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "luxon": "^2.5.0",
    "next": "^15.2.3",
    "node": "^23.10.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.1.0",
    "sass": "^1.56.1",
    "sharp": "^0.29.3",
    "slugify": "^1.6.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@next/eslint-plugin-next": "^16.0.3",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "17.0.5",
    "@types/react": "^19.0.12",
    "@types/sharp": "^0.29.5",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^9.39.1",
    "husky": "^7.0.4",
    "linkinator": "^3.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.2.2"
  }
}

{
  "name": "tritonse.github.io",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "generate-images": "ts-node images/generate.ts",
    "dev": "concurrently \"next dev\" \"chokidar \\\"images/*/**\\\" -c \\\"npm run generate-images\\\"\" \"npm run generate-images\"",
    "build": "npm run generate-images && next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint-fix": "(eslint --fix --cache --report-unused-disable-directives . || true) && prettier --write .",
    "lint-check": "tsc && eslint --cache --report-unused-disable-directives . && prettier --check .",
    "link-test": "linkinator out/ --skip 'dynamicsperformanceteam.com|linkedin.com|ssa.ucsd.edu|jointheleague.org|sdcta.org'",
    "prepare": "husky install .husky"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^12.3.4",
    "@types/luxon": "^2.4.0",
    "@types/mdx": "^2.0.3",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "luxon": "^2.5.0",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.3",
    "sass": "^1.56.1",
    "sharp": "^0.30.5",
    "slugify": "^1.6.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.4",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@types/sharp": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "linkinator": "^3.1.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
